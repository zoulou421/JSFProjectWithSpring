<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

     
 <hibernate-mapping>
    <class name="com.formationkilo.dto.LieuDTO" table="lieux" catalog="javagreenapp">
        <id name="id" type="java.lang.Integer">
            <column name="	LIEU_ID" />
            <generator class="identity" />
        </id>
               
        <property  name="ForeignKeyModForId" type="java.lang.Integer">
            <column name="MODFOR_ID" unique="false"  />
        </property>
 
        <property  name="latitude" type="string">
            <column name="LATITUDE" length="50" not-null="false" unique="false" />
        </property>
        
        <property name="longitude" type="string">
            <column name="LONGITUDE	" length="50" not-null="false" unique="false" />
        </property>        
        <property name="moduleFormationedBy" type="string">
            <column name="MODULE_FORMATION_ED_BY" length="255" not-null="false" unique="false" />
        </property>      
        <property name="moduleFormationedDate" type="string">
            <column name="MODULE_FORMATION_ED_DATE" length="50" not-null="false" unique="false" />
        </property> 
        
        <property name="description" type="string">
            <column name="DESCRIPTION" length="255" not-null="false" unique="false" />
        </property> 
             
    </class>
</hibernate-mapping>    
      
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
     