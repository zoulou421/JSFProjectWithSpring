<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.formationkilo.dto.ModuleFormation" table="moduleformation" catalog="javagreenapp">
        <id name="guid" type="java.lang.Integer">
            <column name="MODFOR_ID" />
            <generator class="identity" />
        </id>
        <property name="type" type="string">
            <column name="TYPE" length="255" not-null="false" unique="false" />
        </property>
        <property name="langage" type="string">
            <column name="LANGAGE" length="255" not-null="false" unique="false" />
        </property>        
        <property name="description" type="string">
            <column name="DESCRIPTION" length="255" not-null="false" unique="false" />
        </property>      
        <property name="lieu" type="string">
            <column name="LIEU" length="255" not-null="false" unique="false" />
        </property>      
    </class>
</hibernate-mapping>